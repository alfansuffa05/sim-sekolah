#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
    int a;
    cout<<"indeks nilai dalam huruf"<<endl;
    cout<<"masukan nilai ";
    cin>>a;
    cout<<endl;
    {
    if(a>=80 && a<=100)
    cout<<"A";
    {
    if(a>=72 && a<80)
    cout<<"B+";
    if(a>=62 && a<72)
    cout<<"B";
    if(a>=55 && a<62)
    cout<<"C+";
    }
    }
 getch();
}
